<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.frontend.controllers.Controller">
   <children>
      <TabPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <tabs>
            <Tab closable="false" text="Список параметров">
               <content>
                  <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                   <columns>
                     <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                     <TableColumn fx:id="typeColumn" prefWidth="75.0" text="Тип" />
                     <TableColumn fx:id="labelColumn" prefWidth="75.0" text="Имя" />
                     <TableColumn fx:id="descriptionColumn" prefWidth="75.0" text="Описание" />
                     <TableColumn fx:id="addressColumn" prefWidth="75.0" text="Адрес" />
                     <TableColumn fx:id="sizeColumn" prefWidth="75.0" text="Размер" />
                     <TableColumn fx:id="valueColumn" prefWidth="75.0" text="Значение" />
                   </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </Tab>
            <Tab closable="false" text="Поиск">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <top>
                        <HBox prefHeight="56.0" prefWidth="526.0" BorderPane.alignment="CENTER">
                           <children>
                              <ComboBox prefWidth="150.0" promptText="Поле">
                                 <HBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </HBox.margin>
                              </ComboBox>
                              <ComboBox prefHeight="26.0" prefWidth="74.0" promptText="==">
                                 <HBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </HBox.margin>
                              </ComboBox>
                              <TextField prefHeight="26.0" prefWidth="105.0" promptText="Значение">
                                 <HBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </HBox.margin>
                              </TextField>
                              <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="88.0" text="Поиск">
                                 <HBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                     </top>
                     <center>
                        <TableView fx:id="table1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="324.0" prefWidth="526.0" BorderPane.alignment="CENTER">
                           <columns>
                              <TableColumn fx:id="idColumn1" prefWidth="75.0" text="ID" />
                              <TableColumn fx:id="typeColumn1" prefWidth="75.0" text="Тип" />
                              <TableColumn fx:id="labelColumn1" prefWidth="75.0" text="Имя" />
                              <TableColumn fx:id="descriptionColumn1" prefWidth="75.0" text="Описание" />
                              <TableColumn fx:id="addressColumn1" prefWidth="75.0" text="Адрес" />
                              <TableColumn fx:id="sizeColumn1" prefWidth="75.0" text="Размер" />
                              <TableColumn fx:id="valueColumn1" prefWidth="75.0" text="Значение" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </center>
                  </BorderPane>
               </content></Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
