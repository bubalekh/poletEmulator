<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.frontend.controllers.Controller">
   <children>
      <TabPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <tabs>
            <Tab closable="false" text="Список параметров">
               <content>
                  <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                   <columns>
                     <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                     <TableColumn fx:id="typeColumn" prefWidth="75.0" text="Тип" />
                     <TableColumn fx:id="labelColumn" prefWidth="75.0" text="Имя" />
                     <TableColumn fx:id="descriptionColumn" prefWidth="75.0" text="Описание" />
                     <TableColumn fx:id="addressColumn" prefWidth="75.0" text="Адрес" />
                     <TableColumn fx:id="sizeColumn" prefWidth="75.0" text="Размер" />
                     <TableColumn fx:id="valueColumn" prefWidth="75.0" text="Значение" />
                   </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </Tab>
            <Tab closable="false" text="Поиск">
               <content>
                  <SplitPane dividerPositions="0.5">
                    <items>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                           <children>
                              <TextField alignment="TOP_LEFT" layoutX="56.0" layoutY="150.0" promptText="SomeText" AnchorPane.leftAnchor="55.0" AnchorPane.rightAnchor="55.0" />
                              <Button layoutX="56.0" layoutY="200.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="150.0" text="Поиск" AnchorPane.leftAnchor="55.0" AnchorPane.rightAnchor="55.0" />
                           </children>
                        </AnchorPane>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                           <children>
                              <ComboBox layoutX="11.0" layoutY="14.0" prefHeight="25.0" prefWidth="235.0" promptText="SomeText" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" />
                           </children></AnchorPane>
                    </items>
                  </SplitPane>
               </content></Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
